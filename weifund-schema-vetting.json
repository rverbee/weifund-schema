{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "vetting_root",
  "type": "object", 
  "properties": {
    "submitter_name": { "type": "string" },
    "SCS_name": { "type": "string" },
    "repository": { "type": "string" },
    "commit_hash": { "type": "string" },
    "readiness_score": { "type": "string" },
    "test_measures": { "$ref": "test_measures" }
  },
  "required": [ 
    "submitter_name",
    "SCS_name",
    "repository",
    "readiness_score"
  ]
}
