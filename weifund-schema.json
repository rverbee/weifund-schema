{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "@context": {
      "type": "string"
    },
    "@type": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "compiler": {
      "type": "string"
    },
    "compilerFlags": {
      "type": "string"
    },
    "compilerVersion": {
      "type": "string"
    },
    "bytecode": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "abi": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "constant": {
            "type": "boolean"
          },
          "inputs": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "indexed": {
                  "type": "boolean"
                },
                "name": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                }
              },
              "required": [
                "indexed",
                "name",
                "type"
              ]
            }
          },
          "name": {
            "type": "string"
          },
          "outputs": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "type": {
                  "type": "string"
                }
              }
            }
          },
          "type": {
            "type": "string"
          },
          "anonymous": {
            "type": "boolean"
          }
        },
        "required": [
          "constant",
          "inputs",
          "name",
          "outputs",
          "type",
          "anonymous"
        ]
      }
    },
    "transactionHash": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "image": {
      "type": "string"
    },
    "mainEntity": {
      "type": "string"
    },
    "i18n": {
      "type": "object",
      "properties": {
        "en": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "alternateName": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "disambiguatedDescription": {
              "type": "string"
            },
            "about": {
              "type": "string"
            },
            "contributionDisclaimer": {
              "type": "string"
            },
            "payoutDisclaimer": {
              "type": "string"
            },
            "refundDisclaimer": {
              "type": "string"
            }
          }
        }
      }
    },
    "mailChimp": {
      "type": "object",
      "properties": {
        "forms": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "@context": {
                "type": "string"
              },
              "@type": {
                "type": "string"
              },
              "action": {
                "type": "string"
              },
              "fields": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "number"
                  }
                }
              }
            }
          }
        }
      }
    },
    "relatedContracts": {
      "type": "array",
      "items": {}
    }
  }
}